                config KERNEL_CC_STACKPROTECTOR_REGULAR
                        bool "Regular"
                config KERNEL_CC_STACKPROTECTOR_STRONG
                        depends on GCC_VERSION_4_9_LINARO
                        bool "Strong"
        endchoice
