+	{"IPAddress",		dalType_string,	0,	0,	NULL},
+	{"IPLinkLocalAddress6",		dalType_string,	0,	0,	NULL},
+	{"PhysAddress",		dalType_string,	0,	0,	NULL},
+	{"AddressSource",		dalType_string,	0,	0,	NULL},
+	{"X_ZYXEL_ConnectionType",		dalType_string,	0,	0,	NULL},
